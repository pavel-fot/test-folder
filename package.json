{
  "name": "sn-front.invite-app",
  "version": "1.13.1",
  "description": "Invite App for SignNow",
  "main": "src/index.js",
  "scripts": {
    "clear": "rm -rf node_modules; rm -rf lib; rm -f package-lock.json",
    "managers": "npm install --prefix ../ sn-front-webpack sn-front-eslint sn-front-jest",
    "init:lint": "cross-env PATH=\"../node_modules/.bin:$PATH\" sn-front-eslint",
    "init:build": "cross-env PATH=\"../node_modules/.bin:$PATH\" sn-front-webpack",
    "flow": "cross-env PATH=\"../node_modules/.bin:$PATH\" flow",
    "flow:cov": "cross-env PATH=\"../node_modules/.bin:$PATH\" flow coverage",
    "compile": "cross-env PATH=\"../node_modules/.bin:$PATH\" babel src --out-dir lib",
    "test": "cross-env PATH=\"../node_modules/.bin:$PATH\" jest --coverage",
    "build": "cross-env PATH=\"../node_modules/.bin:$PATH\" webpack",
    "start": "cross-env PATH=\"../node_modules/.bin:$PATH\" webpack-dev-server",
    "lint": "cross-env PATH=\"../node_modules/.bin:$PATH\" eslint ./src && echo \"No errors!\"",
    "mount": "cp -a ./public/* $WEBAPI_PUBLIC/react-app"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SignNowInc/sn-front.invite-app"
  },
  "author": "Artyom Shaitor <artyomshaitor@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/SignNowInc/sn-front.invite-app/issues"
  },
  "homepage": "https://github.com/SignNowInc/sn-front.invite-app#deploy",
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hotkeys": "^0.10.0",
    "react-redux": "^5.0.6",
    "react-universal-component": "^2.8.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "cross-env": "^5.2.0",
    "history": "^4.7.2",
    "moment": "^2.22.2",
    "query-string": "^5.1.0",
    "react-bootstrap": "^1.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-input-mask": "^2.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "connected-react-router": "^6.5.2",
    "react-scroll-to-component": "^1.0.2",
    "react-sortable-hoc": "^0.7.4",
    "redux-logger": "^3.0.6",
    "redux-modal-dispatcher": "^1.3.7",
    "reselect": "^3.0.1",
    "sn-front-react-vendors": "^2.0.0",
    "sn-front-static": "^15.20.24",
    "sn-front-webpack-config": "^3.81.4",
    "sn-front.alerts-manager": "^1.2.5",
    "sn-front.api-client": "^1.4.0",
    "sn-front.api-endpoints": "^2.53.1",
    "sn-front.controls": "^3.45.2",
    "sn-front.doc-groups": "^1.0.4",
    "sn-front.doc-list": "^1.30.0",
    "sn-front.folder-tree": "^2.6.1",
    "sn-front.modals": "^1.54.2",
    "sn-front.profile-modal": "^1.4.1",
    "sn-front.progress-indicator": "^2.0.1",
    "sn-front.redux-utils": "^2.29.3",
    "sn-front.scroll-bars": "^1.0.1",
    "sn-front.user": "^1.2.1",
    "tinycon": "^0.6.8"
  }
}
